<div class="uk-grid" style="margin-top: 75px;" uk-grid>
  <div class="uk-width-1-1">
    <div class="action-bar">
      <div>
        <h3>Adresse de Livraison</h3>
      </div>
      <div>
        <a href="/shipment" class="uk-button uk-button-default uk-text-capitalize">
          <span uk-icon="icon: pencil; ratio: 1.1"></span>
          Modifier
        </a>
      </div>
    </div>
  </div>
  <div class="uk-width-1-2@s" style="margin-top: 0px;">
    <div class="ui-text-field">Prénom</div>
    <input name="firstname" id="firstname" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.firstname %>" disabled>
  </div>
  <div class="uk-width-1-2@s" style="margin-top: 0px;">
    <div class="ui-text-field">Nom</div>
    <input name="lastname" id="lastname" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.lastname %>" disabled>
  </div>
  <div class="uk-width-1-2@s">
    <div class="ui-text-field">Téléphone</div>
    <input name="phone" id="phone" class="ui-input uk-input uk-form-medium" type="number" value="<%= shipmentAddress.phone %>" disabled>
  </div>
  <div class="uk-width-1-1@s">
    <div class="ui-text-field">Adresse</div>
    <input name="address" id="address" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.address %>" disabled>
  </div>
  <div class="uk-width-1-2@s">
    <div class="ui-text-field">Code Postal</div>
    <input name="postalcode" id="postalcode" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.postalcode %>" disabled>
  </div>
  <div class="uk-width-1-2@s">
    <div class="ui-text-field">Ville</div>
    <input name="city" id="city" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.city %>" disabled>
  </div>
  <div class="uk-width-1-2@s">
    <div class="ui-text-field">État</div>
    <input name="state" id="state" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.state %>" disabled>
  </div>
  <div class="uk-width-1-2@s">
    <div class="ui-text-field">Pays</div>
    <input name="country" id="country" class="ui-input uk-input uk-form-medium" type="text" value="<%= shipmentAddress.country %>" disabled>
  </div>
</div>
